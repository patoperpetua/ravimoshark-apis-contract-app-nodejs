{
  "name": "ravimoshark-contract-api",
  "version": "1.0.0",
  "description": "This is an API to interact with Ravimoshark contracts database. It allows to create, modify and delete contracts and machines. It also interacts with machines imported from SAGE database. It does not contain any kind of user validation but It requests to have a valid authentication user to interact with it. For more information about user Authentication, please refer to [Ravimoshark User Authentication](https://ravimoshark.com/back/apis/auth/latest).",
  "main": "src/index.js",
  "scripts": {
    "check": "npm-check",
    "postinstall": "./scripts/init_service.sh d",
    "start-ts": "ts-node src/server.ts -r dotenv/config",
    "start": "npm run tsc && node build/server.js",
    "lint": "tslint --project ./",
    "tsc": "rm -rf build && tsc",
    "docker-generate": "./scripts/docker/docker_image_generate.sh y",
    "docker-run": "./scripts/docker/docker_compose_run.sh p",
    "docker-run:dev": "./scripts/docker/docker_compose_run.sh d",
    "generateEntities": "typeorm-model-generator -h d05qizmaog.database.windows.net -d db_taches_new -u XXXX -x XXXX -p 1433 -e mssql -o src/databases/entities --noConfig --generateConstructor --ssl",
    "generateTest": "oatts generate -s src/api/swagger.yaml -w test",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "test": "mocha -r ts-node/register --recursive test"
  },
  "keywords": [
    "swagger",
    "openapi",
    "ravimoshark",
    "singletonSD",
    "contracts"
  ],
  "homepage": "https://gitlab.com/ravimosharksas/apis/contract/app",
  "license": "SEE LICENSE IN LICENSE file",
  "bugs": {
    "url": "https://gitlab.com/ravimosharksas/apis/contract/app/issues",
    "email": "patricio.perpetua.arg@gmail.com"
  },
  "author": {
    "name": "Patricio Perpetua",
    "email": "patricio.perpetua.arg@gmail.com",
    "url": "http://patricioperpetua.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ravimosharksas/apis/contract/app.git"
  },
  "private": true,
  "dependencies": {
    "@hapi/joi": "16.1.4",
    "@types/bcrypt": "3.0.0",
    "@types/cors": "2.8.5",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "8.3.2",
    "@types/lodash": "4.14.132",
    "@types/node": "12.0.3",
    "@types/swagger-tools": "0.10.6",
    "bcrypt": "3.0.6",
    "connect": "3.7.0",
    "cors": "2.8.5",
    "credential": "2.0.0",
    "dotenv": "8.0.0",
    "express": "^4.17.1",
    "express-jwt": "5.3.1",
    "js-yaml": "3.13.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.15",
    "mssql": "5.1.0",
    "normalize-port": "1.0.0",
    "oas-tools": "^2.1.7",
    "oas3-tools": "^1.0.1",
    "path-to-regexp": "3.0.0",
    "reflect-metadata": "0.1.13",
    "shelljs": "0.8.3",
    "swagger-tools": "0.10.4",
    "typeorm": "0.2.19",
    "typescript": "3.5.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",

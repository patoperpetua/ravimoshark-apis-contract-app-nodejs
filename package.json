    "@types/mocha": "5.2.7",
    "chai-http": "4.3.0",
    "chakram": "1.5.0",
    "mocha": "6.1.4",
    "nodemon": "1.18.11",
    "npm-check": "^5.9.0",
    "oatts": "1.3.0",
    "request": "2.88.0",
    "supertest": "4.0.2",
    "ts-node": "8.2.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "5.16.0",
    "typeorm-model-generator": "^0.3.5",
    "z-schema": "4.0.2"
  }
}
